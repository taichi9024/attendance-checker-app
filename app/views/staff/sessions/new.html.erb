<h1>Staff login page</h1>
<%= form_with model: @form, url: :staff_session do |f|%>
    <div class="form-group">
    <%= f.label :name, "名前" %>
    <%= f.text_field :name, class:"form-control" %>
    </div>

    <div class="form-group">
    <%= f.label :pass, "パスワード" %>
    <%= f.password_field :pass, class:"form-control" %>
    </div>
    <%= f.submit class:"btn btn-primary" %>
<% end %>
