<h1>個人情報変更</h1>

<%= form_with model: @staff, url: :staff_changes do |f| %>
    <div class="form-group">
    <%=f.label :name, "名前" %>
    <%= f.text_field :name, class:"form-control" %>
    </div>

    <div class="form-group">
    <%=f.label :name, "パス" %>
    <%= f.password_field :pass, class:"form-control" %>
    <%= f.submit class:"btn btn-primary" %>
<% end %>
